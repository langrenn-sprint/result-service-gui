[flake8]
select = ANN,A50,B,B9,BLK,C,D,DAR,E,F,I,S,W,N
docstring-convention = google
max-complexity = 21
ignore = ANN001,ANN101,ANN401,BLK100,B009,E203,E501,W503
max-line-length = 120
per-file-ignores =
    __init__.py: F401
    tests/*: S101,E800
application-import-names = result_service_gui, tests
import-order-style = google
